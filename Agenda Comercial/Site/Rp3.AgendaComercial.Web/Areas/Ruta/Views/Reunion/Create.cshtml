@model Rp3.AgendaComercial.Models.Ruta.Reunion
@{
    ViewBag.Title = Rp3.AgendaComercial.Resources.TitleFor.CrearReunion;
    this.SetImportFormElements(true);
    this.SetImportFormValidations(true);
    this.SetImportTables(true);
}

@Html.DevExpress().GetScripts(
    new Script { ExtensionSuite = ExtensionSuite.HtmlEditor },
    new Script { ExtensionSuite = ExtensionSuite.Editors }
)

@Html.DevExpress().GetStyleSheets(
            new StyleSheet { ExtensionSuite = ExtensionSuite.Editors, Theme = "MetropolisBlue" }
)

@using (Html.Rp3BeginForm())
{
    using (Html.Rp3BeginContentBox(Rp3.AgendaComercial.Resources.TitleFor.Reunion))
    {
        @:@{Html.RenderPartial("_Datos", Model);}


        @Html.Rp3FormActionGroup(ActivityActions.Create)
    }
}

<script>
    setTimeout(function () {
        $('#Estado').select2("enable", false);
    }, 100);
</script>
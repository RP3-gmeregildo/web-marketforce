@model IEnumerable<Rp3.AgendaComercial.Models.Ruta.ProgramacionTarea>
@{
    ViewBag.Title = Rp3.AgendaComercial.Resources.TitleFor.ProgramacionTareas;
}

@section ActionTitle{
    @Html.Rp3OptionActivityButton("PROGRAMATAREA", "NEW", HtmlElementSize.Default, includeText: true, applicationId: "AGENDACOMERCIAL")
}

@Html.DevExpress().GetScripts(
    new Script { ExtensionSuite = ExtensionSuite.GridView }
)

@Html.DevExpress().GetStyleSheets(
            new StyleSheet { ExtensionSuite = ExtensionSuite.GridView, Theme = "MetropolisBlue" }
)

@Html.Partial("_GridViewIndex", Model)

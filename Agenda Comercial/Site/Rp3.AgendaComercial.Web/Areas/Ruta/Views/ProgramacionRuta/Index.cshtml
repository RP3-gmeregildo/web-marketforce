@model IEnumerable<Rp3.AgendaComercial.Models.Ruta.ProgramacionRuta>
@{
    ViewBag.Title = Rp3.AgendaComercial.Resources.TitleFor.ProgramacionRutas;
}

@section ActionTitle{
    @Html.Rp3OptionActivityButton("PROGRAMARUTA", "NEW", HtmlElementSize.Default, includeText: true, applicationId: "AGENDACOMERCIAL")
}

@Html.DevExpress().GetScripts(
    new Script { ExtensionSuite = ExtensionSuite.GridView }
)

@Html.DevExpress().GetStyleSheets(
            new StyleSheet { ExtensionSuite = ExtensionSuite.GridView, Theme = "MetropolisBlue" }
)

@Html.Partial("_GridViewIndex", Model)


@model IEnumerable<Rp3.AgendaComercial.Models.General.Canal>
@{
    ViewBag.Title = Rp3.AgendaComercial.Resources.TitleFor.Canales;    
    this.SetImportAdvTables(true);
}

@section ActionTitle{
    @Html.Rp3OptionActivityButton("CANAL", "NEW", HtmlElementSize.Default, includeText: true, applicationId: "AGENDACOMERCIAL")

@Html.Rp3ActionButton(Rp3.AgendaComercial.Resources.LabelFor.Excel, "ExportToXls", "Canal", new { Area = "General" }, themeType: ElementThemeType.Default, icon: Icons.File)
@Html.Rp3ActionButton(Rp3.AgendaComercial.Resources.LabelFor.Pdf, "ExportToPdf", "Canal", new { Area = "General" }, themeType: ElementThemeType.Default, icon: Icons.File)
}


@using (Html.Rp3BeginColumn(HtmlColumnSize.Size_12))
{
        @Html.Partial("_GridViewIndex", Model)            
}

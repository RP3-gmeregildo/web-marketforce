@model List<Rp3.AgendaComercial.Models.Marcacion.View.AgenteGrupo>
@{

}

<div id="searchagente_content_dialog_form">

    @using (Html.Rp3BeginDataTable(scrollY: "350px", allowSort: false, allowPagination: false, allowSearch: true, sortColumn:0, htmlAttributes: new { id = "searchagentetable" }))
    {
        <thead>
            <tr>
                <th>@Rp3.AgendaComercial.Resources.LabelFor.AgenteGrupo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var agente in Model)
            {
                <tr class="selectable-item">
                    <td idAgente="@agente.IdAgente" idGrupo="@agente.IdGrupo" descripcion="@agente.Descripcion">@agente.Descripcion</td>
                </tr>
            }
        </tbody>
    }

</div>

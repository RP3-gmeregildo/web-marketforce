<script>
    RP3_ROOT_PATH = "@Url.Content("~")";
    if (RP3_ROOT_PATH.length > 0) {
        var valueString = RP3_ROOT_PATH.substring(RP3_ROOT_PATH.length - 1, RP3_ROOT_PATH.length);
        if (valueString == '/')
            RP3_ROOT_PATH = RP3_ROOT_PATH.substring(0, RP3_ROOT_PATH.length - 1);
    }
</script>
